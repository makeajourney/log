[centos@ansible-host ~]$ cat get_yum_installed_list.yml 
---
- hosts: targets
  become: yes
  user: centos
  strategy: free

  tasks:
    - name: get file containing installed list
      command: /tmp/jum6 flist installed base {{ ansible_eth0.ipv4.address }}

    - name: fetch files from remote
      fetch: src=/tmp/{{ ansible_eth0.ipv4.address }}.json dest=/tmp/{{ ansible_eth0.ipv4.address }}.json flat=yes
      
[centos@ansible-host ~]$ cat move_jum6.yml 
---
- hosts: targets
  become: yes
  user: centos

  tasks:
    - name: move py module jum6
      copy: src=jum6 dest=/tmp/jum6 mode=777
